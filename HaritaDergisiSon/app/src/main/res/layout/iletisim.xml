        android:layout_marginTop="4dp"
        android:text=" 0 312 595 23 28"/>

    <TextView
        android:id="@+id/haritacilikmüzesi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tel6"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="10dp"
        android:text="Haritacılık Müzesi"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tel7"
        android:layout_below="@id/haritacilikmüzesi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="4dp"
        android:text=" 0 312 595 23 38"/>






</RelativeLayout>
</ScrollView>