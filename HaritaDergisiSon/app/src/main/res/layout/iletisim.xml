        android:text="  0 312 320 14 95"/>

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/faks"
        android:layout_marginTop="4dp"
        android:layout_marginLeft="3dp"
        android:text=" hgm@harita.gov.tr" />


    <TextView
        android:id="@+id/donerSermaye"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
